package shopbanhang.Controller.User;

import org.springframework.stereotype.Repository;

import shopbanhang.Dao.BaseDao;
import shopbanhang.Entity.Users;

@Repository
public class UsersDao extends BaseDao {
	public int AddAccount(Users user) {
		StringBuilder sqlBuffer = new StringBuilder();
		sqlBuffer.append("INSERT INTO users (user, password, display_name, address) VALUES (");
		sqlBuffer.append("[value-2], "); // Thay [value-2] bằng giá trị thích hợp
		sqlBuffer.append("[value-3], "); // Thay [value-3] bằng giá trị thích hợp
		sqlBuffer.append("[value-4], "); // Thay [value-4] bằng giá trị thích hợp
		sqlBuffer.append("[value-5])"); // Thay [value-5] bằng giá trị thích hợp
		String sqlStatement = sqlBuffer.toString();
		int insert = 0;
		return insert;
	}
}
