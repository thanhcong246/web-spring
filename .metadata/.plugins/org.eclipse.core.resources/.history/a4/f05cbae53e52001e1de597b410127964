import org.springframework.stereotype.Repository;

import shopbanhang.Entity.Slides;

@Repository
public class HomeDao {

	@Autowired
	public JdbcTemplate _jdbcTemplate;
	public List<Slides> list = new ArrayList<Slides>();
}
