		productsDto.setId_product(rs.getLong("id"));
		productsDto.setId_product(rs.getLong("id"));
		productsDto.setId_product(rs.getLong("id"));
		productsDto.setId_product(rs.getLong("id"));
		productsDto.setId_product(rs.getLong("id"));
		productsDto.setId_product(rs.getLong("id"));
		productsDto.setId_product(rs.getLong("id"));
		productsDto.setId_product(rs.getLong("id"));
